{
  "name": "user_managment",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node --env-file=.env server.js",
    "dev": "conc 'npm:watch:*'",
    "watch:server": "cls && node --watch --watch-path=src --env-file=.env server.js",
    "watch:css": "cls && postcss public/css/tailwind.css -o public/css/styles.css --watch",
    "check": "npx @biomejs/biome check --apply \"server.js\" \"src\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@biomejs/biome": "^1.7.2",
    "autoprefixer": "^10.4.19",
    "chalk": "^5.3.0",
    "concurrently": "^8.2.2",
    "cssnano": "^7.0.1",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.3"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-ejs-layouts": "^2.5.1",
    "http-errors": "^2.0.0",
    "mongoose": "^8.3.3",
    "zod": "^3.23.6"
  }
}
